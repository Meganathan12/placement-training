#include <stdio.h>

int main() {
      int a;
      int b;
      int c;
      int maximum;
      printf("Enter a number ");
      scanf("%d %d %d",&a,&b,&c);
      if(a>b){
          if(a>c){
                   maximum = a;
              }else{
                  maximum= c;
              }
              }else if(b>c){
                      maximum = b;
                  }else{
                      maximum= c;
                      
                  }
printf("%d", maximum);
